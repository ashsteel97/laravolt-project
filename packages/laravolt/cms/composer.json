{
    "name": "laravolt/cms",
    "description": "Flexible content management system for Laravel",
    "keywords": [
        "laravel",
        "laravolt",
        "blog",
        "CMS",
        "content"
    ],
    "homepage": "https://github.com/laravolt/cms",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "bayu.hendra@javan.co.id",
            "homepage": "http://id-laravel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.1.3",
        "illuminate/database": "5.6.*|5.7.*",
        "illuminate/support": "5.6.*|5.7.*",
        "spatie/laravel-sluggable": "^2.1",
        "spatie/laravel-tags": "^2.1",
        "spatie/laravel-translatable": "^2.2",
        "spatie/laravel-schemaless-attributes": "^1.1",
        "kalnoy/nestedset": "^4.3",
        "laravolt/suitable": "^2.6",
        "mad-web/laravel-enum": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "orchestra/testbench": "~3.0",
        "orchestra/database": "~3.0",
        "codedungeon/phpunit-result-printer": "^0.19.14"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Cms\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/database/migrations"
        ],
        "psr-4": {
            "Laravolt\\Cms\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravolt\\Cms\\ServiceProvider"
            ]
        }
    }
}
